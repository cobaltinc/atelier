  "license": "MIT",
  "description": "",
  "engines": {
    "node": ">=14.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cobaltinc/atelier"
  },
  "scripts": {
    "build": "lerna run build --stream --prefix --npm-client yarn",
    "clean": "lerna run clean && lerna clean --yes && rimraf node_modules",
    "format": "prettier --write '**/*.{js,md,scss,ts,tsx}' '!**/{dist,storybook}/**'",
    "format:diff": "prettier --list-different '**/*.{js,md,scss,ts,tsx}' '!**/{dist,storybook}/**'",
    "lint": "tslint packages",
    "lint:docs": "alex 'packages/**/*.md' -q",
    "type-check": "tsc",
    "storybook": "start-storybook -p 6006"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@babel/plugin-proposal-export-namespace-from": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-syntax-class-properties": "^7.12.1",
    "@babel/plugin-syntax-decorators": "^7.12.1",
    "@babel/plugin-syntax-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-async-to-generator": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/register": "^7.12.10",
    "@babel/runtime": "^7.12.5",
    "@parcel/babel-plugin-transform-runtime": "nightly",
    "@parcel/babel-preset-env": "nightly",
    "@parcel/transformer-inline": "nightly",
    "@parcel/transformer-svg-react": "nightly",
    "@parcel/transformer-typescript-types": "nightly",
    "@storybook/addon-actions": "6.2.9",
    "@storybook/addon-links": "6.2.9",
    "@storybook/addon-storysource": "6.2.9",
    "@storybook/addon-viewport": "6.2.9",
    "@storybook/addons": "6.2.9",
    "@storybook/preset-scss": "1.0.3",
    "@storybook/react": "6.2.9",
    "@testing-library/react": "11.2.6",
    "@types/react": "17.0.4",
    "@types/react-dom": "17.0.3",
    "alex": "^8.0.0",
    "babel-plugin-react-remove-properties": "0.3.0",
    "babel-plugin-transform-glob-import": "1.0.1",
    "css-loader": "5.2.4",
    "lerna": "^3.20.2",
    "parcel": "nightly",
    "prettier": "1.19.1",
    "react": "17.0.2",
    "react-docgen-typescript-loader": "3.7.2",
    "react-dom": "17.0.2",
    "sass": "1.32.12",
    "sass-loader": "10.1.1",
    "style-loader": "2.0.0",
    "tslint": "6.0.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-react": "4.2.0"
  },
